{
  "name": "setup-mold",
  "version": "1.0.0",
  "description": "Github Action to setup mold (https://github.com/rui314/mold) as default system linker",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "ncc build -o dist index.js"
  },
  "env": {
    "RUNNER_TOOL_CACHE": "tmp/cache",
    "RUNNER_TEMP": "tmp/temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Warchant/setup-mold.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Warchant/setup-mold/issues"
  },
  "homepage": "https://github.com/Warchant/setup-mold#readme",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/io": "^1.1.1",
    "@actions/tool-cache": "^1.7.1",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.33.1"
  },
  "volta": {
    "node": "16.14.0"
  }
}
